{
  "name": "resume",
  "description": "My Resume",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "build:styles": "./node_modules/.bin/node-sass --recursive --output public --source-map true --source-map-contents app/styles",
    "build:app": "./node_modules/.bin/babel -d ./build ./app -s",
    "build": "npm run lint && npm run clean:build && npm run build:app && npm run build:styles",
    "clean:build": "rimraf build && rimraf public",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "lint:eslint": "eslint .",
    "lint:sass-lint": "sass-lint -c ./.sass-lint.yml './app/**/*.scss' -v",
    "lint": "npm run lint:eslint && npm run lint:sass-lint",
    "production-build": "npm run build && npm run clean:node_modules && npm install --production",
    "production": "NODE_ENV=production node ./build/index.js",
    "reinstall": "npm run clean && npm install",
    "start": "./node_modules/.bin/nodemon --exec NODE_ENV=development ./node_modules/.bin/babel-node ./app/index.js"
  },
  "dependencies": {
    "consolidate": "0.14.5",
    "debug-caller": "2.2.0",
    "express": "4.15.3",
    "git-rev-sync": "1.9.1",
    "node-sass-middleware": "0.11.0",
    "swig": "1.4.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2"
  }
}
